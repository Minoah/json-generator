{
  "name": "mrr-json-generator",
  "version": "1.0.4",
  "description": "Generate JSON data samples based on a template, including multiple data types and options",
  "main": "out/index.js",
  "scripts": {
    "test": "ts-node src/index -i template.json -o output.json",
    "todos": "ts-node src/index -i template-todos.json -o output-todos.json",
    "comments": "ts-node src/index -i template-comments.json -o output-comments.json"
  },
  "author": "Marcos Rivas Rojas",
  "license": "ISC",
  "bin": {
    "json": "out/index.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "@types/uuid": "^8.0.0",
    "express": "^4.17.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/yargs": "^15.0.5",
    "uuid": "^8.2.0",
    "yargs": "^15.4.1"
  }
}
